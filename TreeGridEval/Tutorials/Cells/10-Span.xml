<Grid>
   <Cfg id="Span" NoVScroll="1" NoHScroll="1" IdChars="0123456789" NumberId="1" MainCol="N" FastPanel="0" CopyPasteRows="2"/>
   <Def>
      <D Name="R" Spanned="1" A='A' B='B' C='C' D='D' E='E' F='F' G='G' H='H' I='I' J='J'/>
      <D Name='C' Width='30'/>
   </Def>
   <Panel Spanned='1'/>
   <Header SortIcons='2'/>
   <Cols>
      <C Name="Panel"/>
      <C Name="id" Type="Int" CanEdit="0" CanSort="0"/>
      <C Name="N" Width="260"/>
      <C Name="A" Group="1"/>
      <C Name="B" Group="1"/>
      <C Name="C" Group="1"/>
      <C Name="D" Group="1"/>
      <C Name="E" Group="1"/>
      <C Name="F"/>
      <C Name="G" Spanned="1"/>
      <C Name="H" Spanned="1" Width="32"/>
      <C Name="I" Spanned="1" Group="2" Width="37"/>
      <C Name="J" Group="2" Width="37"/>
   </Cols>
   <Body>
      <B>
         <I id="0"/>
         <I id="1" PanelSpan="2"
            N="Span - column span" 
            ASpan="5"
            GRowSpan="3" GRotate="1" G="RowSpan"
            HRowSpan="3.5" HRotate="1" H="RowSpan in tree&#x0a;in Lines with CRLF" HWrap="0" HType="Lines" HAlign="Right"
            />
         <I id="2"
            PanelRowSpan="2.5"
            N="Merge - show different column"
            ASpan="5" AMerge="D"
            IRowSpan="3" ISpan="2" I="RowSpan&#x0a;and Span" IType="Lines"
            />
         <I id="3"
            N="Merge - show all spanned values"
            ASpan="5" AMerge="A,B,C,D,E"
            >
            <I id="4"
               N="MergeEdit - edit different column"
               ASpan="5" AMerge="E,B,C" AMergeEdit="B"
               />
            <I id="5" MidHtml="Custom spanned HTML"/>
            <I id="6"
               N="MergeFormat - exact display format"
               ASpan="5" AMerge="A,B,C,D,E" AMergeFormat="A&lt;b>://&lt;/b>B&lt;b>/&lt;/b>C&lt;b>#&lt;/b>D&lt;b>?&lt;/b>E" AMergeEditFormat="A://B/C#D?E"
               />
         </I>
         <I id="7"
            N="MergeEditFormat - format for editing"
            ASpan="5" AMerge="A,C,D,E" AMergeFormat="C = (E+D) * A" AMergeEditFormat="E+D"
            />
      </B>
   </Body>
   <Header id="id" N="Description"/>
</Grid>