<Grid>
   <!-- Translations to other languages -->

   <!-- German -->

   <TextDE>
      <Set Row='Toolbar,Toolbar1' WinLabelRight='Fenster scrollen' WinTip='Scrollbalken des Grids deaktivieren und die der Webseite verwenden'/>
      <Set Row='ID' Panel='Panel' id='ID' Index='Index'/>
      <Set Row='Panel' Panel='Panel'/>
      <Set Row='Header' Index='Index'/>
      <ReplaceText Col='Menu'
         Rows='Zeilen' Columns='Spalten' Select='Auswählen' Show='Einblenden' Hide='Ausblenden' Add='Neu' Copy='Kopieren' Delete='Löschen'
