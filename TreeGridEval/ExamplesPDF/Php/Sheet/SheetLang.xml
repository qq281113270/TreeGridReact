   </TextDE>
   <Media>
      <M Language='DE'>
         <Cfg PrintPagePrefix="&lt;center class='%9' style='width:%7px'>Beispiel 'Sheet', Seite %3 von %6&lt;/center>" PrintPagePostfix="&lt;center class='%9' style='width:%7px'>Horizontal: Seite %1 von %4 ,Vertikal: Seite %2 von %5&lt;/center>"/>
      </M>
   </Media>


