   <TextXX>
      <Set Row='Toolbar,Toolbar1' WinLabelRight='!Window scroll!' WinTip='!Disable grid scrollbars and use page scrollbars!'/>
      <Set Row='ID' Panel='!Panel!' id='!ID!'/>
      <Set Row='Panel' Panel='!Panel!'/>
