         <Cfg PrintPagePostfix="&lt;center style='width:%7px'>صفحه %1 افقي از %4 , صفحه %2 عمودي  از %5&lt;/center>" PrintPagePrefix="&lt;center style='width:%7px'>صفحه %3 از %6&lt;/center>"/>
